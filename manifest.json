{
    "manifest_version": 3,
    "name": "Max Autolytics Provisioner",
    "version": "2.0",
    "icons": {
      "16": "autolytics_logo.png",
      "48": "autolytics_logo.png",
      "128": "autolytics_logo.png"
    },
    "description": "A provisioner tool for Max Autolytics with vAuto Provision",
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["activeTab", "scripting", "storage", "onStartup", "onActivated", "tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_title": "Max Autolytics Provisioner",
      "default_icon": "autolytics_logo.png"
    },
    "host_permissions": [
      "https://www2.vauto.com/Va/Inventory/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://www2.vauto.com/Va/Inventory/"],
        "js": ["webhook_interact.js"]
      }
    ]
  }